{{>header}}

{{#if @root.exportCore}}
export { ApiError } from './core/ApiError';
export { OpenAPI } from './core/OpenAPI';
export type { OpenAPIConfig } from './core/OpenAPI';
{{/if}}
{{#if @root.exportModels}}
{{#if models}}

export type { OperationsType, ParametersType } from './models/_symbols';
{{#each models}}
export type { {{{name}}} } from './models/{{{name}}}';
{{/each}}
{{/if}}
{{/if}}
{{#if @root.exportSchemas}}
{{#if models}}

{{#each models}}
export { ${{{name}}} } from './schemas/${{{name}}}';
{{/each}}
{{/if}}
{{/if}}
{{#if @root.exportClients}}
{{#if services}}

{{#each services}}
export * from './services/{{{name}}}{{{@root.postfix}}}';
{{/each}}
{{/if}}
{{/if}}
{{#if @root.exportOperations}}
{{#if services}}

{{#each services}}
{{#each operations}}
export type { {{{pascalCase name}}}Operation } from './operations/{{{../name}}}{{{@root.postfix}}}';
{{/each}}
{{/each}}
{{/if}}
{{/if}}
