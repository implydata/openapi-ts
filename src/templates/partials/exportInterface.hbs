{{#ifdef description deprecated}}
/**
{{#if description}}
 * {{{escapeComment description}}}
{{/if}}
{{#if deprecated}}
 * @deprecated
{{/if}}
 */
{{/ifdef}}
export type {{{name}}} = {
	{{#each properties}}
	{{#ifdef description deprecated}}
	/**
	{{#if description}}
	 * {{{escapeComment description}}}
	{{/if}}
	{{#if deprecated}}
	 * @deprecated
	{{/if}}
	 */
	{{/ifdef}}
	{{>isReadOnly}}{{{name}}}{{>isRequired}}: {{>type parent=../name}};
	{{/each}}
};
